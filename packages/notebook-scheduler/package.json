{
  "name": "@aiworkspace/notebook-scheduler-extension",
  "version": "0.4.0",
  "description": "JupyterLab extension - Enable submitting notebooks as batch jobs on external runtimes",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/ai-workspace/ai-workspace",
  "bugs": {
    "url": "https://github.com/ai-workspace/ai-workspace/issues"
  },
  "license": "Apache-2.0",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-workspace/ai-workspace"
  },
  "scripts": {
    "build": "tsc",
    "dist": "npm pack .",
    "clean": "rimraf lib && rimraf node_modules && rimraf yarn.lock && rimraf yarn-error.log && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/notebook": "^1.0.0",
    "@phosphor/disposable": "^1.1.2",
    "@phosphor/widgets": "^1.8.0",
    "json-loader": "^0.5.7",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "typescript": "~3.5.2"
  },
  "jupyterlab": {
    "extension": true
  },
  "publishConfig": {
    "registry": "https://na.artifactory.swg-devops.com/artifactory/api/npm/dbg-aiw-npm-virtual/"
  }
