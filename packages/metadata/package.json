{
  "name": "@elyra/metadata-extension",
  "version": "3.2.2",
  "description": "JupyterLab extension - View and edit metadata",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/elyra-ai/elyra",
  "bugs": {
    "url": "https://github.com/elyra-ai/elyra/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elyra-ai/elyra/"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "dist": "npm pack .",
    "prepare": "npm run build",
    "watch": "tsc -w",
    "lab:install": "jupyter labextension install --no-build",
    "lab:uninstall": "jupyter labextension uninstall --no-build"
  },
  "dependencies": {
    "@elyra/metadata-common": "3.2.1",
    "@elyra/services": "3.2.1",
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/codeeditor": "^3.0.0",
    "@jupyterlab/ui-components": "^3.0.0",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/widgets": "^1.17.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
