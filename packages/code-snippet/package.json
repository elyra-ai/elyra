{
  "name": "@elyra/code-snippet-extension",
  "version": "0.9.0-dev",
  "description": "JupyterLab extension - Reusable code snippets for your Notebook and Python Scripts",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/elyra-ai/elyra",
  "bugs": {
    "url": "https://github.com/elyra-ai/elyra/issues"
  },
  "license": "Apache-2.0",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/elyra-ai/elyra/"
  },
  "scripts": {
    "build": "tsc",
    "dist": "npm pack .",
    "clean": "rimraf lib && rimraf node_modules && rimraf yarn.lock && rimraf yarn-error.log && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@elyra/application": "^0.9.0-dev",
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/cells": "^1.0.0",
    "@jupyterlab/codeeditor": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docmanager": "^1.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/fileeditor": "^1.0.0",
    "@jupyterlab/mainmenu": "^1.0.0",
    "@phosphor/algorithm": "^1.1.3",
    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/messaging": "^1.2.3",
    "@phosphor/signaling": "^1.3.0",
    "@phosphor/widgets": "^1.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "rimraf": "^2.6.3",
    "typescript": "~3.5.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
