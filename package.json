{
  "name": "elyra",
  "version": "0.8.0-dev",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "eslint": "eslint . --fix --ignore-path .gitignore --ext .ts,.tsx",
    "eslint:check": "eslint . --ignore-path .gitignore --ext .ts,.tsx",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json}\"",
    "prettier:check": "prettier --ignore-path .gitignore --check \"**/*{.ts,.tsx,.js,.jsx,.css,.json}\"",
    "test-start-server": "jupyter lab --debug --LabApp.token='test' --no-browser --port-retries=0",
    "test": "start-server-and-test test-start-server http-get://localhost:8888?token=test cy:run",
    "test-debug": "start-server-and-test test-start-server http-get://localhost:8888?token=test cy:open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^1.3.1",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "~2.23.0",
    "@typescript-eslint/parser": "~2.23.0",
    "cypress": "^4.1.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^2.3.0",
    "lerna": "^3.16.4",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.3",
    "start-server-and-test": "^1.10.9",
    "ts-loader": "^6.2.1",
    "typescript": "~3.5.2",
    "webpack": "^4.42.0"
  },
  "resolutions": {
    "react": "~16.8.6",
    "@types/react": "~16.8.6",
    "react-dom": "~16.8.5",
    "@types/react-dom": "~16.8.5",
    "react-intl": "^2.3.0",
    "@types/react-intl": "^2.3.0"
  }
}
