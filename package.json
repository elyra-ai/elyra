{
  "name": "elyra",
  "version": "2.3.0-dev",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "lerna run build --stream",
    "start": "run-s build:ui lab:install:ui lab:build lab:start",
    "watch": "run-p watch:*",
    "watch:lab": "run-s build:ui lab:install:ui lab:watch",
    "watch:ui": "lerna run watch --parallel",
    "lab:start": "jupyter lab",
    "lab:watch": "jupyter lab --watch",
    "lab:ci": "mkdirp build/cypress-tests && jupyter lab --config=./tests/test-config.py > /dev/null 2> build/cypress-tests/jupyter.log",
    "lab:install": "run-p lab:install:*",
    "lab:install:ui": "lerna run lab:install --stream",
    "lab:build": "jupyter lab build",
    "lab:uninstall": "run-s lab:uninstall:*",
    "lab:uninstall:server": "pip uninstall -y elyra jupyterlab-git jupyter-lsp jupyterlab-lsp python-language-server jupyter-resource-usage",
    "lab:uninstall:ui": "jupyter labextension uninstall --all",
    "lab:clean": "jupyter lab clean",
    "lint": "run-s lint:*",
    "lint:server": "flake8 elyra",
    "lint:ui": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "format": "prettier --ignore-path .gitignore --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json}\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*{.ts,.tsx,.js,.jsx,.css,.json}\"",
    "test": "run-s test:server test:ui",
    "test:server": "pytest elyra",
    "test:ui": "lerna run test --concurrency 1 --stream",
    "test:integration": "start-server-and-test lab:ci http-get://localhost:58888?token=test cy:run",
    "test:integration:debug": "start-server-and-test lab:ci http-get://localhost:58888?token=test cy:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "clean": "git clean -Xdf"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "@blueprintjs/core": "^3.36.0",
    "@blueprintjs/select": "^3.15.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "~17.0.0",
    "@types/react-intl": "^3.0.0",
    "@lumino/widgets": "1.17.0",
    "cypress": "^6.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^3.0.0"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^1.3.1",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@jupyterlab/testutils": "3.0.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "~2.23.0",
    "@typescript-eslint/parser": "~2.23.0",
    "cypress": "^6.2.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^2.3.0",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "jest-raw-loader": "^1.0.1",
    "lerna": "^3.16.4",
    "lint-staged": "^9.5.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "~3.0.2",
    "start-server-and-test": "1.7.9",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.2.2",
    "typescript": "~4.1.3",
    "webpack": "^5.0.0"
  }
}
